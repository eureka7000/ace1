<body>

<div id="loading" style="display:none">
    <img src="/assets/img/Loading.gif" />
</div>

<div class="wrapper">
    <%= render partial: 'modals/notice_email_confirm' if current_user.confirmed_at.nil? %>
    <!--=== Header ===-->
    <div class="header">

        <div class="container" style="height: 100px;">

            <!-- Logo -->
            <a class="logo" href="/">
                <img src="/assets/img/logo.jpg" alt="Logo" class="img-responsive">
            </a>
            <!-- End Logo -->

            <!-- Topbar -->
            <div class="topbar">

                <ul class="loginbar pull-right">
                    <% unless current_user.nil? %>
                        <% unless current_user.study_histories.last.nil? %>
                            <li>
                                <a class="text-blue" href="/contents/<%= current_user.study_histories.last.unit_concept_id %>"  title="최근학습">
                  <span class="label rounded-2x label-success" style="margin-left: 5px">
                      <i class="fa fa-bookmark" style="color:white"></i>
                      <%=  current_user.study_histories.last.unit_concept.name %>
                  </span>
                                </a>
                            </li>
                            <li class="topbar-devider"></li>
                        <% end %>

                        <li>
                            <% if current_user.can_I_use? %>
                                <span class="label rounded-2x label-blue" style="margin-left: 5px">Member</span>
                            <% else %>
                                <span class="label rounded-2x label-yellow" style="margin-left: 5px" title="Level1까지 자유롭게 사용가능">Free</span>
                            <% end %>
                        </li>
                        <li class="topbar-devider"></li>

                        <li><span class="label rounded-2x label-dark-blue"><%= current_user.user_types[0].user_type.humanize %></span></li>
                        <li class="topbar-devider"></li>

                        <li>
                            <a rel="nofollow" data-method="delete" href="/users/sign_out">
                  <span class="label rounded-2x label-dark">
                      <i class="icon-logout"></i>
                      로그아웃
                  </span>
                            </a>
                        </li>

                    <% else %>

                        <li>
                            <a href="/users/sign_in">
                  <span class="label rounded-2x label-dark">
                      <i class="icon-login"></i>
                      <%= t(:log_in) %>
                  </span>
                            </a>
                        </li>
                        <li class="topbar-devider"></li>

                        <li><a href="/users/sign_up"><span class="label rounded-2x label-purple"><%= t(:sign_up) %></span></a></li>

                    <% end %>

                </ul>

            </div>
            <!-- End Topbar -->

            <!-- Toggle get grouped for better mobile display -->
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="fa fa-bars"></span>
            </button>
            <!-- End Toggle -->
        </div><!--/end container-->

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse navbar-responsive-collapse margin-right-10">

            <div class="container">

                <ul class="nav navbar-nav margin-right-10">
                    <li class="<%= 'active' if @active == 'home' %> dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown">STUDY</a>
                        <ul class="dropdown-menu">
                            <li><a href="/contents?view_type=1&step=1">단원별</a></li>
                            <li><a href="/contents?view_type=2&step=1">학년별</a></li>
                            <!--<li><a href="/exams/previous_exams">기출문제</a></li>-->
                            <li><a href="/homes/contents_list">학습 목차 한눈에 보기</a></li>
                            <!-- <li><a href="/contents/by_level">난위도별</a></li> -->
                        </ul>
                    </li>

                    <li class="<%= 'active' if @active == 'mypage' %>">
                        <% if @session_yn == 'Y' %>
                            <% if current_user.user_types[0].user_type == 'school teacher' || 'mento' %>
                                <a href="/mypages/user_info">MY PAGE&nbsp;<span class="label rounded label-danger pull-right"><%= @questions_number unless @questions_number == 0 %></span></a>
                            <% end %>
                        <% else %>
                            <a href="/mypages/overall">MY PAGE</a>
                        <% end %>
                    </li>

                    <li class="<%= 'active' if @active == 'blog' %> dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown">BLOG</a>
                        <ul class="dropdown-menu">
                            <li>
                                <a href="/questions">학습문제해결</a>
                            </li>
                            <!--<li>
                              <a href="/blogs/succession_case">성공사례</a>
                            </li>-->
                            <li>
                                <a href="/blogs/math_story">수학이야기</a>
                            </li>
                        </ul>

                    <li class="<%= 'active' if @active == 'about' %> dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown">ABOUT</a>

                        <ul class="dropdown-menu">
                            <li>
                                <a href="/blogs/faq">FAQ</a>
                            </li>
                            <li>
                                <a href="/blogs/notice">공지사항</a>
                            </li>
                            <!--<li>
                              <a href="/blogs/the_news">새소식</a>
                            </li>-->
                            <li>
                                <a href="/blogs/contact_us">Contact us</a>
                            </li>

                            <!-- 회사소개 -->
                            <li class="dropdown-submenu">
                                <a href="javascript:void(0);">&nbsp;&nbsp;&nbsp;회사소개</a>
                                <ul class="dropdown-menu submenu-left">
                                    <li><a href="/blogs/recommendation">- 추천사</a></li>
                                    <li><a href="/blogs/company_introduction">- 유레카매스란?</a></li>
                                </ul>
                            </li>
                            <!-- End 회사소개 -->

                        </ul>
                    </li>
                    <!-- 임시 주석 처리
                      <li class="<%= 'active' if @active == 'payments' %>">
                        <a href="/payments">상품결제</a>
                      </li>
                      -->
                </ul>
            </div><!--/end container-->
        </div><!--/navbar-collapse-->

    </div>
    <!--=== End Header ===-->

    <% unless @page_name.nil? %>
        <!--=== Breadcrumbs ===-->
        <div class="breadcrumbs">
            <div class="container">
                <h1 class="pull-left"><%= @page_name %></h1>
                <ul class="pull-right breadcrumb">
                    <li><a href="index.html">Home</a></li>
                    <li class="active"><%= @page_name %></li>
                </ul>
            </div><!--/container-->
        </div><!--/breadcrumbs-->
        <!--=== End Breadcrumbs ===-->
    <% end %>

    <%= yield %>

</div><!--/wrapper-->

<!-- JS Global Compulsory -->
<script type="text/javascript" src="/assets/plugins/jquery/jquery.min.js"></script>
<script type="text/javascript" src="/assets/plugins/jquery/jquery-migrate.min.js"></script>
<script type="text/javascript" src="/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
