<p id="notice"><%= notice %></p>

<h1>Listing Study Histories</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Unit concept</th>
      <th>Segment</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_histories.each do |study_history| %>
      <tr>
        <td><%= study_history.user_id %></td>
        <td><%= study_history.unit_concept_id %></td>
        <td><%= study_history.segment %></td>
        <td><%= study_history.status %></td>
        <td><%= link_to 'Show', study_history %></td>
        <td><%= link_to 'Edit', edit_study_history_path(study_history) %></td>
        <td><%= link_to 'Destroy', study_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study history', new_study_history_path %>
