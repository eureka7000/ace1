<div class="col-md-12">
    <h2 class="margin-bottom-20"><%= @year %>년 기출문제</h2>
    <% Exam::ORG_ORG.each_pair do |key, value| %>
        <div class="col-md-6">
            <div class="service-block service-block-<%= value %> rounded-4x" onclick="goShow('<%= key %>', '<%= @year %>')">
                <i class="icon-custom icon-color-light rounded-x icon-line icon-diamond"></i>
                <h2 class="heading-md"><%= key %></h2>
            </div>
        </div>
    <% end %>
</div>

<script type="text/javascript">
    function goShow(org, year) {
        var org = org;
        var year = year;

        var url = '/exams/previous_exams?org='+org+'&year='+year;
        location = url;
    }
</script>